<codetext>
public void runChildren(NavigationView navigation_view) {
		ITreeNode[] children = getChildren();
		for(int i=0; i&amp;lt;children.length;i++) {
			if(!children[i].isNotRunInBatch())
				children[i].runChildren(navigation_view);
		}
		//-- теперь для самого 
		run(navigation_view);
	}


	public void runAsChild(NavigationView navigation_view) {
		if(!isNotRunInBatch())
			run(navigation_view);
	}
</codetext>
<footer>
&amp;lt;/pre&amp;gt;&amp;lt;br/&amp;gt;&amp;lt;br/&amp;gt;
